{
  "name": "ai-calorie-tracker-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for AI Calorie Tracker",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^13.1.0",
    "firebase-functions": "^6.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "private": true
}
